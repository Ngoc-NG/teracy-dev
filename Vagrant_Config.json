{
  "box":"opscode-ubuntu-1204",
  "box_url":"https://opscode-vm-bento.s3.amazonaws.com/vagrant/opscode_ubuntu-12.04-i386_chef-11.4.4.box",
  "forwarded_ports":[
    {
      "guest":8000,
      "host":8000
    }
  ],
  "synced_folders":[
    {
      "host":"./workspace",
      "guest":"/home/vagrant/workspace",
      "mount_options":[
        "dmode=777",
        "fmode=777"
      ]
    },
    {
      "host":"./home/.virtualenvs",
      "guest":"/home/vagrant/.virtualenvs"
    },
    {
      "host":"./home/.ssh",
      "guest":"/home/vagrant/.ssh",
      "mount_options":[
        "dmode=775",
        "fmode=600"
      ]
    }
  ],
  "chef_cookbooks":[
    "vendor-cookbooks",
    "main-cookbooks"
  ],
  "chef_role":"roles",
  "chef_bags_path":"data_bags",
  "chef_recipes":[
    "apt",
    "vim",
    "git",
    "teracy-dev"
  ],
  "forward_agent":true,
  "workspace":[
    "/vagrant/workspace/readonly",
    "/vagrant/workspace/upstream",
    "/vagrant/workspace/personal"
  ],
  "git":{
    "user":{
      "name":"Teracy Dev",
      "email":"teracy-dev@teracy.com"
    },
    "color":true,
    "commit":{
      "template":true
    },
    "diff":{
      "tool":"vimdiff"
    },
    "merge":{
      "tool":"vimdiff"
    },
    "difftool":{
      "prompt":false
    }
  },
  "nodejs":{
    "enabled":true,
    "npm":[
      "grunt-cli",
      "bower"
    ]
  },
  "python":{
    "enabled":true,
    "pip":{
      "global":{

      }
    }
  },
  "ruby":{
    "enabled":false
  },
  "java":{
    "enabled":false,
    "version":"7",
    "flavor":"openjdk",
    "maven":{
      "enabled":false,
      "version":"3"
    }
  },
  "gettext":false
}